{
  "extends": ["airbnb", "airbnb/hooks"],
  "plugins": [
    // ...
    "react-hooks"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-process-exit": "off",
    "object-shorthand": "off",
    "class-methods-use-this": "off",
    "linebreak-style": [
      "error",
      "windows"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ]
  }
}
